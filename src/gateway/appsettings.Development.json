{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "services-tracking",
    "seed": true
  },
  "rabbitmq": {
    "Namespace": "gateway",
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "Port": 5672,
    "Hostnames": [ "rabbitmq" ],
    "RequestTimeout": "00:00:10",
    "PublishConfirmTimeout": "00:00:01",
    "RecoveryInterval": "00:00:10",
    "PersistentDeliveryMode": true,
    "AutoCloseConnection": true,
    "AutomaticRecovery": true,
    "TopologyRecovery": true,
    "Exchange": {
      "Durable": true,
      "AutoDelete": true,
      "Type": "Topic"
    },
    "Queue": {
      "AutoDelete": true,
      "Durable": true,
      "Exclusive": true
    }
  },
  //"trackingServiceUrl": "http://localhost:5004",
  //"customersServiceUrl": "http://localhost:5000",
  //"vehiclesServiceUrl": "http://localhost:5002",
  "trackingServiceUrl": "http://acc.services.tracking:80",
  "customersServiceUrl": "http://acc.services.customers:80",
  "vehiclesServiceUrl": "http://acc.services.vehicles:80",
  "AllowedHosts": "*"
}